    <record id="ir_action_server_action_message_sales_person" model="ir.actions.server">
        <field name="name">Message for sales person</field>
        <field name="model_id" ref="base.model_res_partner" />
        <field name="state">code</field>
        <field name="code">
            record.message_post(body='%s is interested for partner.' % record.name)
        </field>
    </record>

    <record id="marketing_campaign_1" model="marketing.campaign">
        <field name="name">Commercial prospection</field>
        <field name="state">draft</field>
        <field name="model_id" ref="base.model_res_partner" />
    </record>

    <record id="marketing_activity_1" model="marketing.activity">
        <field name="name">Offer free catalog</field>
        <field name="mass_mailing_id" ref="mass_mailing_offer_free_trial" />
        <field name="campaign_id" ref="marketing_campaign_1"/>
    </record>

    <record id="marketing_activity_2" model="marketing.activity">
        <field name="name">After 7 days (if not yet customers)</field>
        <field name="mass_mailing_id" ref="mass_mailing_after_7_days" />
        <field name="interval_number">7</field>
        <field name="interval_type">days</field>
        <field name="activity_domain">[['category_id', 'not ilike', 'Partner']]</field>
        <field name="campaign_id" ref="marketing_campaign_1"/>
    </record>

    <record id="marketing_activity_3" model="marketing.activity">
        <field name="name">Message for sales person</field>
        <field name="interval_number">1</field>
        <field name="interval_type">hours</field>
        <field name="trigger_type">mail_reply</field>
        <field name="activity_type">action</field>
        <field name="server_action_id" ref="ir_action_server_action_message_sales_person" />
        <field name="parent_id" ref="marketing_activity_2" />
        <field name="campaign_id" ref="marketing_campaign_1"/>
    </record>

    <record id="marketing_campaign_1" model="marketing.campaign">
        <field name="marketing_activity_ids" eval="[(4, ref('marketing_activity_1')), (4, ref('marketing_activity_2')), (4, ref('marketing_activity_3'))]" />
    </record>

