        <field name="groupby">account_id</field>
        <field name="formulas">balance = sum.balance</field>
        <field name="parent_id" ref="account_financial_report_l10n_fi_bs_line_1_1_1"/>
    </record>
    <record id="account_financial_report_l10n_fi_bs_line_1_1_1_3" model="account.financial.html.report.line">
