<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Finland Balance Sheet -->
    <record id="account_financial_report_l10n_fi_bs" model="account.financial.html.report">
        <field name="name">Balance Sheet</field>
        <field name="unfold_all_filter" eval="True"/>
        <field name="date_range" eval="False" />
        <field name='parent_id' ref='l10n_fi.account_reports_fi_statements_menu'/>
    </record>

    <record id="account_financial_report_l10n_fi_bs_line_1" model="account.financial.html.report.line">
        <field name="name">Vastaavaa</field>
        <field name="level" eval="0"/>
        <field name="sequence" eval="1"/>
        <field name="formulas">balance = FIBS_ASS_A.balance + FIBS_ASS_B.balance</field>
        <field name="financial_report_id" ref="account_financial_report_l10n_fi_bs"/>
    </record>
    <record id="account_financial_report_l10n_fi_bs_line_1_1" model="account.financial.html.report.line">
        <field name="name">A) PYSYVÄT VASTAAVAT</field>
        <field name="code">FIBS_ASS_A</field>
        <field name="level" eval="1"/>
        <field name="sequence" eval="1"/>
        <field name="formulas">balance = FIBS_ASS_AI.balance + FIBS_ASS_AII.balance + FIBS_ASS_AIII.balance</field>
        <field name="parent_id" ref="account_financial_report_l10n_fi_bs_line_1"/>
    </record>
    <record id="account_financial_report_l10n_fi_bs_line_1_1_1" model="account.financial.html.report.line">
        <field name="name">I. Aineettomat hyödykkeet</field>
        <field name="code">FIBS_ASS_AI</field>
        <field name="level" eval="2"/>
        <field name="sequence" eval="1"/>
        <field name="formulas">balance = FIBS_ASS_AI_1.balance + FIBS_ASS_AI_2.balance + FIBS_ASS_AI_3.balance + FIBS_ASS_AI_4.balance + FIBS_ASS_AI_5.balance</field>
        <field name="parent_id" ref="account_financial_report_l10n_fi_bs_line_1_1"/>
    </record>
    <record id="account_financial_report_l10n_fi_bs_line_1_1_1_1" model="account.financial.html.report.line">
        <field name="name">1. Kehittämismenot</field>
        <field name="code">FIBS_ASS_AI_1</field>
        <field name="level" eval="3"/>
        <field name="sequence" eval="1"/>
        <field name="domain" eval="[('account_id.tag_ids', 'in', [ref('l10n_fi.account_tag_development_costs')])]"/>
        <field name="groupby">account_id</field>
        <field name="formulas">balance = sum.balance</field>
        <field name="parent_id" ref="account_financial_report_l10n_fi_bs_line_1_1_1"/>
    </record>
    <record id="account_financial_report_l10n_fi_bs_line_1_1_1_2" model="account.financial.html.report.line">
