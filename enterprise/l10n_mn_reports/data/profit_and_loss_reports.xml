    <record id="report_line_retained_earn" model="account.financial.html.report.line">
        <field name="code">MNPL_TOTPROFBT</field>
        <field name="name">Татвар төлөхийн өмнөх ашиг (алдагдал)</field>
        <field name="sequence" eval="20"/>
        <field name="level" eval="2"/>
        <field name="formulas">balance = (MNPL_GRP.balance + MNPL_RENT.balance + MNPL_INTER.balance + MNPL_DIVID.balance + MNPL_ROYAL.balance + MNPL_OIN.balance) - (MNPL_SEXP.balance + MNPL_AEXP.balance + MNPL_FEXP.balance + MNPL_OEXP.balance) + (MNPL_EXCHANGE.balance + MNPL_ASTGAIN.balance + MNPL_INASTGAIN.balance + MNPL_INVSTGAIN.balance + MNPL_OTHGAIN.balance)</field>
        <field name="parent_id" ref="report_profit_root"/>
        <field name="special_date_changer">normal</field>
    </record>
