        <field name="sequence" eval="1"/>
        <field name="level" eval="3"/>
        <field name="formulas">balance = MN_TPAY.balance + MN_SPAY.balance + MN_TXPAY.balance + MN_SIFPAY.balance + MN_STLOAN.balance + MN_LIPAY.balance + MN_DIVPAY.balance + MN_PREINC.balance + MN_RESLIA.balance + MN_OCLIA.balance + MN_DEPLIA.balance</field>
