"                    <tbody>\n"
"                        <tr><td style=\"padding:15px 20px 10px 20px;\">\n"
"                            <p>Sayın ${object.partner_id.name}\n"
"                                % if object.partner_id.parent_id:\n"
"                                    (${object.partner_id.parent_id.name})\n"
"                                % endif\n"
"                            </p>\n"
"                            <p>\n"
"                                İşte, ekte,\n"
"                                % if object.name:\n"
"                                   fatura <strong>${object.name}</strong>\n"
"                                % else:\n"
"                                    invoice\n"
"                                %endif\n"
"                                % if object.invoice_origin:\n"
"                                    (referansla: ${object.invoice_origin} )\n"
"                                % endif\n"
"                                miktarında<strong>${format_amount(object.amount_total, object.currency_id)}</strong>\n"
