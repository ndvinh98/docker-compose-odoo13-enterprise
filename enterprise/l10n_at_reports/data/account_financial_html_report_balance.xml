        <field name="parent_id" ref="account_financial_report_l10n_at_paragraph_224_ugb_line_root"/>
        <field name="sequence" eval="1"/>
        <field name="name">Aktiva</field>
        <field name="code">A</field>
        <field name="formulas">balance = AA.balance + AB.balance + AC.balance + AD.balance</field>
        <field name="domain" eval="False" />
        <field name="groupby">account_id</field>
        <field name="level" eval="1" />
        <field name="special_date_changer">normal</field>
    </record>

