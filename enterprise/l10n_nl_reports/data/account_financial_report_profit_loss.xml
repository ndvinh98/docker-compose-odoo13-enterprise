            <field name="formulas">balance = -sum.balance</field>
            <field name="parent_id" ref="account_financial_report_pnl_1_1_1"/>
            <field name="domain" eval="[('account_id.tag_ids', 'in', ref('l10n_nl.account_tag_16'))]"/>
